{
  "name": "libris",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 5173",
    "build": "pnpm dlx prisma generate --schema=./prisma/schema/ && next build",
    "postinstall": "pnpm dlx prisma generate --schema=./prisma/schema/",
    "start": "next start --port 5173",
    "lint": "next lint",
    "db:generate": "prisma generate --schema=./prisma/schema/",
    "db:push": "prisma db push --schema=./prisma/schema/",
    "db:studio": "prisma studio --schema=./prisma/schema/",
    "db:seed": "prisma db seed --schema=./prisma/schema/"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "better-auth": "^1.2.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "lucide-react": "^0.511.0",
    "next": "14.2.29",
    "next-themes": "^0.4.6",
    "prisma": "^6.8.2",
    "react": "^18",
    "react-dom": "^18",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "sharp": "^0.34.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.62",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.29",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
